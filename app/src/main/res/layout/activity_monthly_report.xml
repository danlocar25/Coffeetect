<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
xmlns:app="http://schemas.android.com/apk/res-auto"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@drawable/green_background"
tools:context=".MonthlyReport">

  <ListView
      android:id="@+id/diseaseListView"
      android:layout_width="320dp"
      android:layout_height="148dp"
      android:background="@color/white"
      app:layout_constraintBottom_toBottomOf="@+id/imageView5"
      app:layout_constraintEnd_toEndOf="@+id/imageView5"
      app:layout_constraintStart_toStartOf="@+id/imageView5"
      app:layout_constraintTop_toTopOf="@+id/imageView5"
      app:layout_constraintVertical_bias="0.73"
      tools:ignore="MissingConstraints" />

  <ImageView
      android:id="@+id/imageView52"
      android:layout_width="370dp"
      android:layout_height="90dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="1.0"
      app:srcCompat="@drawable/login_white_backg9" />

  <androidx.constraintlayout.widget.ConstraintLayout
      android:id="@+id/linearLayout6"
      android:layout_width="370dp"
      android:layout_height="65dp"
      android:orientation="horizontal"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.48"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.98"
      tools:ignore="MissingConstraints">

    <ImageButton
        android:id="@+id/monthly"
        android:layout_width="53dp"
        android:layout_height="44dp"
        android:background="@drawable/baseline_analytics_24"
        android:backgroundTint="#362415"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.91"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.49" />

    <ImageButton
        android:id="@+id/tree"
        android:layout_width="40dp"
        android:layout_height="33dp"
        android:background="@drawable/ictreevisual"
        android:backgroundTint="#00704a"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.71"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/leaf"
        android:layout_width="40dp"
        android:layout_height="33dp"
        android:background="@drawable/leaf_icon"
        android:backgroundTint="#00704a"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.29"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/home"
        android:layout_width="40dp"
        android:layout_height="33dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/ichome"
        android:backgroundTint="#00704a"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.43" />

    <ImageButton
        android:id="@+id/cam"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/capture_ic"
        android:backgroundTint="#00704a"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

  </androidx.constraintlayout.widget.ConstraintLayout>

  <TextView
      android:id="@+id/textView"
      android:layout_width="wrap_content"
      android:layout_height="31dp"
      android:fontFamily="@font/hammersmith_one"
      android:text="This Month Report"
      android:textColor="#FFFFFF"
      android:textSize="20sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.029999971"
      tools:ignore="MissingConstraints" />

  <ImageView
      android:id="@+id/imageView5"
      android:layout_width="355dp"
      android:layout_height="350dp"
      android:layout_marginBottom="200dp"
      android:background="@drawable/calendar1"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.36" />

  <!-- TextView for displaying category and percentage -->


  <com.github.mikephil.charting.charts.PieChart
      android:id="@+id/graphBarChart"
      android:layout_width="313dp"
      android:layout_height="310dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.19999999"
      tools:ignore="MissingConstraints" />


  <Button
      android:id="@+id/clearButton"
      android:layout_width="wrap_content"
      android:layout_height="50dp"
      android:backgroundTint="#FF1100"
      android:fontFamily="@font/hammersmith_one"
      android:text="Clear"
      android:textColor="#FFFFFF"
      android:textSize="15dp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.498"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.84000003" />

  <Button
      android:id="@+id/generatePieChartButton"
      android:layout_width="90dp"
      android:layout_height="50dp"
      android:backgroundTint="@color/white"
      android:fontFamily="@font/hammersmith_one"
      android:text="Load"
      android:textColor="@color/green"
      android:textSize="15dp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.3"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.64" />

  <Button
      android:id="@+id/addButton"
      android:layout_width="90dp"
      android:layout_height="50dp"
      android:backgroundTint="@color/white"
      android:fontFamily="@font/hammersmith_one"
      android:text="Add"
      android:textColor="@color/green"
      android:textSize="15dp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.7"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.64" />

  <ImageButton
      android:id="@+id/legendButton"
      android:layout_width="42dp"
      android:layout_height="42dp"
      android:layout_gravity="end"
      android:layout_margin="16dp"
      android:background="@drawable/legendic2"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.98"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.52" />

  <Button
      android:id="@+id/year"
      android:layout_width="wrap_content"
      android:layout_height="50dp"
      android:backgroundTint="@color/light_blue_A400"
      android:fontFamily="@font/hammersmith_one"
      android:text="Yearly Report"
      android:textColor="@color/white"
      android:textSize="15sp"
      android:textStyle="bold"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.74" />


</androidx.constraintlayout.widget.ConstraintLayout>